{
	"name": "codelab-litelement",
	"version": "1.0.0",
	"description": "Codelab LitElement - A simple base class for creating fast, lightweight web components",
	"scripts": {
		"start": "npm run generate",
		"postinstall": "npm run update:submodule",
		"generate": "speaker-cli generate",
		"generate:slides": "speaker-cli generate --slides",
